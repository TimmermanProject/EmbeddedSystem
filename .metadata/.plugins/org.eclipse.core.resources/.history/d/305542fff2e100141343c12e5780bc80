package shared;

public class RFID extends Frame {
	private int accessCode; 
	
	public RFID (){
		this.setFrameType(frameTypes.RFID_ADD);
	}
	
	/** getters and setters **/
	
	public void setAccesCode(int accessCode){
		this.accessCode = accessCode;
	}
	
	public int getAccessCode (){
		return accessCode;
	}
	
}
